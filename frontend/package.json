{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite dev",
		"build": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"i18n": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.7.0",
		"@skeletonlabs/skeleton": "^3.2.2",
		"@skeletonlabs/skeleton-svelte": "^1.5.3",
		"@tailwindcss/vite": "^4.1.18",
		"artplayer": "^5.3.0",
		"hls.js": "^1.6.15",
		"tailwindcss": "^4.1.18"
	}
}
